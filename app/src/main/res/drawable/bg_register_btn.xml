<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="10dp"/>
            <solid android:color="@color/bg_for_rebtn_pressed"/>
            <stroke android:width="1dp" android:color="@color/stroke_for_rebtn"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <corners android:radius="10dp"/>
            <solid android:color="@color/bg_for_rebtn_unpress"/>
            <stroke android:width="1dp" android:color="@color/stroke_for_rebtn"/>
        </shape>
    </item>
</selector>